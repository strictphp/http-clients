{
  "name": "strictphp/http-clients",
  "type": "project",
  "description": "arious http client implementations for better developer and devops experience.",
  "keywords": [
    "laravel",
    "php",
    "strictphp",
    "http-client"
  ],
  "license": "MIT",
  "homepage": "https://github.com/strictphp/http-clients",
  "authors": [
    {
      "name": "Martin Kluska",
      "email": "martin@kluska.cz"
    },
    {
      "name": "Milan Matějček",
      "email": "milan.matejcek@gmail.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=8.1",
    "guzzlehttp/psr7": "^2.5",
    "illuminate/filesystem": "^9.0 || ^10.0 || ^11.0",
    "psr/container": "^2.0",
    "psr/event-dispatcher": "^1.0",
    "psr/http-client": "^1.0",
    "psr/http-message": "^1.0 || ^2.0",
    "psr/simple-cache": "^3.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.8",
    "phpstan/phpstan-strict-rules": "^1.4",
    "phpstan/phpstan-deprecation-rules": "^1.1.3"
  },
  "scripts": {
    "lint:stan": "vendor/bin/phpstan analyse"
  },
  "autoload": {
    "psr-4": {
      "StrictPhp\\HttpClients\\": "src"
    }
  },
  "autoload-dev": {},
  "extra": {
    "laravel": {
      "providers": []
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
